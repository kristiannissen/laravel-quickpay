{
    "name": "kristiannissen/quickpay",
    "license": "MIT",
    "authors": [
        {
            "name": "Kristian Nissen",
            "email": "kristian.nissen@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "QuickPay\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "QuickPay\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "prettier": "prettier \"src/**/*.php\" --write"
    },
    "extra": {
        "laravel": {
            "providers": [
                "QuickPay\\QuickPayServiceProvider"
            ]
        }
    }
}
